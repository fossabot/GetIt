{
    "manifest_version": 2,
    "name": "Wanna",
    "description": "This extension allows the user to download some stuff.",
    "version": "1.0",
    "background": {
        "scripts": ["wanna.js"],
        "persistent": false
    },
    "browser_action": {
        "default_icon": "icon.png",
        "default_title": "Wanna!"
    },
    "permissions": [
        "activeTab",
        "downloads"
    ],
    "author": "Val Saven <val.saven@gmail.com>"
}
