{
    "manifest_version": 2,
    "name": "Wanna",
    "description": "This extension allows the user to download some stuff.",
    "version": "2.0.0",
    "background": {
        "scripts": ["wanna.js"],
        "persistent": false
    },
    "browser_action": {
        "default_icon": "icon.png",
        "default_title": "Wanna!"
    },
    "permissions": [
        "activeTab",
        "downloads"
    ],
    "icons": {
        "16": "icons/icon16.png",
        "48": "icons/icon48.png",
       "128": "icons/icon128.png"
    },
    "author": "Val Saven <val.saven@gmail.com>"
}
